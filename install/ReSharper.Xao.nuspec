<?xml version="1.0"?>
<package>
  <metadata>
    <id>ReSharper.Xao</id>
    <version>$version$</version>
    <authors>Igal Tabachnik, Andrew Karpov</authors>
    <owners>Igal Tabachnik, Andrew Karpov</owners>
    <title>Xao - Navigate between a XAML View and ViewModel.</title>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://github.com/hmemcpy/ReSharper.Xao/blob/master/LICENSE</licenseUrl>
    <description>A plugin that allows you to quickly navigate between XAML Views/CodeBehind and the corresponding ViewModel files, based on naming convention. 
For example, navigating from CustomerDetailsView to CustomerDetailsViewModel and vice-versa, by pressing the Go to Related Files shortcut (Ctrl+Shift+Alt+G).</description>
    <projectUrl>https://github.com/hmemcpy/ReSharper.Xao</projectUrl>
    <iconUrl>https://raw.github.com/hmemcpy/ReSharper.Xao/master/logo.png</iconUrl>
    <copyright>Copyright &#x00A9; 2014-$currentyear$ Igal Tabachnik</copyright>
    <tags>xaml viewmodel mvvm navigate view model</tags>
    <releaseNotes>$releasenotes$</releaseNotes>
    <dependencies>
      <dependency id="Wave" version="[$wave$]" />
    </dependencies>
  </metadata>
  <files>
    <file src="src\ReSharper.Xao\bin\$configuration$\ReSharper.Xao.*" target="dotFiles"/>
  </files>
</package>